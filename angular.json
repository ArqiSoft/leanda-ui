{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "leanda.ui": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/sass"
              ]
            },
            "assets": [{
                "glob": "**/*",
                "input": "src/assets",
                "output": "/"
              },
              {
                "glob": "silent-renew.html",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "auth.html",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "jenkinsBuildData.json",
                "input": "src",
                "output": "/"
              },
              "src/favicon.ico",
              "src/web.config"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/tether/dist/js/tether.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/signalr/jquery.signalR.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/svg-pan-zoom/dist/svg-pan-zoom.js"
            ]
          },
          "configurations": {
            "local": {
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.local.ts"
              }]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.prod.ts"
              }]
            },
            "dev": {
              "outputHashing": "all",
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.dev.ts"
              }]
            },
            "uat": {
              "outputHashing": "all",
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.uat.ts"
              }]
            },
            "test": {
              "outputHashing": "all",
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.test.ts"
              }]
            },
            "deploy": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.deploy.ts"
              }]
            },
            "deploy-fvc": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.deploy-fvc.ts"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "leanda.ui:build",
            "port": 5555
          },
          "configurations": {
            "local": {
              "browserTarget": "leanda.ui:build:local"
            },
            "production": {
              "browserTarget": "leanda.ui:build:production"
            },
            "dev": {
              "browserTarget": "leanda.ui:build:dev"
            },
            "uat": {
              "browserTarget": "leanda.ui:build:uat"
            },
            "test": {
              "browserTarget": "leanda.ui:build:test"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "leanda.ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/tether/dist/js/tether.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/signalr/jquery.signalR.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/svg-pan-zoom/dist/svg-pan-zoom.js"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "assets": [{
                "glob": "**/*",
                "input": "src/assets",
                "output": "/"
              },
              {
                "glob": "silent-renew.html",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "auth.html",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "jenkinsBuildData.json",
                "input": "src",
                "output": "/"
              },
              "src/favicon.ico",
              "src/web.config"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "leanda.ui-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "leanda.ui:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "leanda.ui",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "dr",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "dr"
    }
  }
}
